{
    "name": "Thycotic Light Theme",
    "semanticHighlighting": true,
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#3cc4e5"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#3cc4e5"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#3cc4e5"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#081F2C"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#1B8099"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#3CC4E5"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#008080"
            }
        },
        {
            "name": "Keys and Properties",
            "scope": [
                "property",
                "variable.other.property",
                "variable.other.property.powershell"
            ],
            "settings": {
                "foreground": "#B3D565"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable",
                "punctuation.definition.variable.powershell",
                "variable.other.readwrite.powershell"
            ],
            "settings": {
                "foreground": "#80bb01"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#3ebeb1"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#F1C400"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#e75955"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {}
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#34A297"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#f1c400"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#e99996"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#E75955"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#efbab9"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#34A297"
            }
        }
    ],
    "colors": {
        "sideBar.foreground": "#b3d565",
        "sideBar.background": "#ffffff",
        "titleBar.activeBackground": "#ffffff",
        "titleBar.activeForeground": "#081f2c",
        "statusBar.foreground": "#f1c400",
        "statusBar.background": "#ffffff",
        "statusBar.debuggingBackground": "#3EBEB1",
        "statusBar.debuggingForeground": "#081f2c",
        "statusBar.noFolderBackground": "#081f2c",
        "activityBar.background": "#ffffff",
        "activityBar.foreground": "#081f2c",
        "activityBarBadge.background": "#081f2c",
        "tab.activeBackground": "#80bb01a0",
        "tab.activeForeground": "#e5f0cc",
        "editor.lineHighlightBackground": "#FCF4DA",
        "editor.selectionBackground": "#F5DD8E",
        "editor.background": "#ffffff",
        "settings.checkboxBorder": "#A9A9A9",
        "settings.dropdownBorder": "#A9A9A9",
        "settings.numberInputBorder": "#A9A9A9",
        "settings.textInputBorder": "#A9A9A9",
        "terminal.background": "#ffffff",
        "terminal.foreground": "#081f2c"
    }
}